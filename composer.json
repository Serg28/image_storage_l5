{
  "name": "linecore/image-storage-laravel",
  "description": "Advanced image, video and document storage package for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Artur (Original Author)",
      "email": "artur@vis-design.com"
    },
    {
      "name": "Linecore Team",
      "email": "support@linecore.dev"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "intervention/image": "^2.7|^3.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "classmap": [
      "src/Models",
      "src/Http/Controllers",
      "src/Migrations"
    ],
    "psr-4": {
      "Linecore\\ImageStorage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Linecore\\ImageStorage\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Linecore\\ImageStorage\\ImageStorageServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
